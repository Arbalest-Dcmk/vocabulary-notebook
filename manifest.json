{
    "name": "Vocabulary Notebook",
    "description": "A chrome extension that can help me to improve my English",
    "version": "1.0",
    "manifest_version": 3,
    "options_page":"/options/index.html",
    "action": {
        "default_popup": "/popup/index.html",
        "default_icon": {
            "16": "/icons/16.png",
            "32": "/icons/32.png",
            "48": "/icons/48.png",
            "128": "/icons/icon.png"
        }
    },
    "icons": {
        "16": "/icons/16.png",
        "32": "/icons/32.png",
        "48": "/icons/48.png",
        "128": "/icons/icon.png"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "css":["content/index.css"],
            "js":["content/index.js"]
        }
    ]
}
